  "bugs": {
    "url": "https://github.com/TomAFrench/matic-proofs/issues"
  },
  "homepage": "https://github.com/TomAFrench/matic-proofs#readme",
  "config": {
    "mnemonics": "clock radar mass judge dismiss just intact mind resemble fringe diary casino"
  },
  "devDependencies": {
